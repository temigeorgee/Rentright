                  <option value="flat / apartment">Flat / Apartment</option>
                  <option value="detached duplex">Detached Duplex</option>
                  <option value="terraced duplex">Terraced Duplex</option>
                  <option value="semi detached duplex">Semi Detached Duplex</option>
                  <option value="detached bungalow">Detached Bungalow</option>
                  <option value="shop">Shop</option>
                  <option value="terraced bungalow">Terraced Bungalow</option>
                </select>
              </div>

              <div class="form-group col-12 col-md-6 col-lg-4 col-xl-3 px-0 px-xl-2">
                <select data-width="100%" data-style="btn-default" data-size="6"
                  class="btn-group form-control selectpicker" name="bathrooms" id="InputBedrooms" required>
