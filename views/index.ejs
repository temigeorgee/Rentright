                <select data-width="100%" data-style="btn-default" data-size="6"
                  class="btn-group form-control selectpicker" name="toilets" id="InputToilets" required>
                  <option selected value="">No of batrooms / toilets</option>
                  <option value="1">1 </option>
                  <option value="2">2 </option>
                  <option value="3">3 </option>
                  <option value="4">4</option>
                  <option value="5">5</option>
                  <option value="6">6</option>
                  <option value="7">7</option>
                </select>
              </div>
              <div class="dropdown form-group">
